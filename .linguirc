{
    "locales": [
        "ar-AR",
        "en-EN",
        "es-ES",
        "fr-FR"
    ],
    "sourceLocale": "en-EN",
    "fallbackLocales": {
        "default": "en-EN"
    },
    "catalogs": [
        {
            "path": "src/locales/{locale}/messages",
            "include": [
                "src"
            ],
            "exclude": [
                "/node_modules/.*./node_modules/",
                "/src/locales/"
            ]
        }
    ],
    "format": "minimal"
}
